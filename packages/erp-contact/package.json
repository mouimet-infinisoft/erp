    "react-dom": "17.x",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.181",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"

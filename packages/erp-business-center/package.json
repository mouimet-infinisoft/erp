{
  "name": "@infini-soft/erp-business-center",
  "version": "1.0.0",
  "description": "Infinicloud Micro Front End App",
  "author": "Infinisoft Inc. <info@infini-soft.com>",
  "homepage": "http://www.infini-soft.com/",
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "start:mock": "set ENV=mock&& umi dev",
    "start:test": "set ENV=test&& umi dev",
    "openapi": "umi openapi",
    "test": "umi-test",
    "types": "tsc --declaration --emitDeclarationOnly",
    "build": "umi build",
    "clean": "rm -rf ./dist ./out ./lib ./src/.umi",
    "start": "umi dev",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-card": "^1.19.1",
    "@ant-design/pro-descriptions": "^1.10.60",
    "@ant-design/pro-field": "^1.34.1",
    "@ant-design/pro-form": "^1.61.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-list": "^1.21.46",
    "@ant-design/pro-skeleton": "^1.0.5",
    "@ant-design/pro-table": "^2.70.0",
    "@infini-soft/kitchensink": "^1.0.4",
    "@infini-soft/logger": "^1.0.0",
    "@infini-soft/utils": "^1.2.0",
    "@umijs/plugin-openapi": "^1.3.2",
    "@umijs/plugin-qiankun": "^2.37.1",
    "@umijs/preset-ui": "^2.2.9",
    "antd": "^4.19.3",
    "rc-field-form": "^1.24.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
